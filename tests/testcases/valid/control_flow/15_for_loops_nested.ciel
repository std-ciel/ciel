// Nested for loops

int main() {
    int sum = 0;

    // Test 1: Simple nested for loops
    for (int i = 0; i < 3; i = i + 1) {
        for (int j = 0; j < 3; j = j + 1) {
            sum = sum + i + j;
        }
    }

    // Test 2: Triple-nested for loops
    for (int i = 0; i < 2; i = i + 1) {
        for (int j = 0; j < 2; j = j + 1) {
            for (int k = 0; k < 2; k = k + 1) {
                sum = sum + 1;
            }
        }
    }

    // Validate: Test1: i+j for all combinations = (0+0)+(0+1)+(0+2)+(1+0)+(1+1)+(1+2)+(2+0)+(2+1)+(2+2) = 0+1+2+1+2+3+2+3+4 = 18
    // Test2: 2*2*2 = 8 iterations = 18 + 8 = 26
    if (sum != 26) return 1;

    return 0;
}
