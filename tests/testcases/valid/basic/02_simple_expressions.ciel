// Basic expressions and assignments

int main() {
    int x = 10;
    int y = 20;
    int z;
    
    // Simple assignment
    z = x;
    z = y;
    z = 0;
    
    // Arithmetic expressions
    z = x + y;
    z = x - y;
    z = x * y;
    z = x / y;
    z = x % y;
    
    // Multiple assignments
    int a = 1;
    int b = 2;
    int c = 3;
    
    return 0;
}
