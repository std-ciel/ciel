// Pointer type declarations and usage

int main() {
    // Basic pointers
    int* p1;
    char* p2;
    float* p3;
    bool* p4;
    unsigned* p5;
    
    // Double pointers
    int** pp1;
    char** pp2;
    float** pp3;
    
    // Triple pointers
    int*** ppp1;
    char*** ppp2;
    
    // Void pointers
    void* vp1;
    void** vp2;
    
    // Pointer initialization
    int* ptr = 0;
    char* str = 0;
    
    return 0;
}
