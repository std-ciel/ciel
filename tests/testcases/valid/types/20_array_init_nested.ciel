// Test multidimensional array initialization with nested braces

int main() {
    // 2D array with nested initializers
    int matrix[2][3] = {{1, 2, 3}, {4, 5, 6}};

    // Validate values
    if (matrix[0][0] != 1) return 1;
    if (matrix[0][1] != 2) return 2;
    if (matrix[0][2] != 3) return 3;
    if (matrix[1][0] != 4) return 4;
    if (matrix[1][1] != 5) return 5;
    if (matrix[1][2] != 6) return 6;

    // 2D char array with nested initializers
    char grid[2][2] = {{'A', 'B'}, {'X', 'Y'}};

    if (grid[0][0] != 'A') return 7;
    if (grid[0][1] != 'B') return 8;
    if (grid[1][0] != 'X') return 9;
    if (grid[1][1] != 'Y') return 10;

    // 3D array
    int cube[2][2][2] = {{{1, 2}, {3, 4}}, {{5, 6}, {7, 8}}};

    if (cube[0][0][0] != 1) return 11;
    if (cube[0][0][1] != 2) return 12;
    if (cube[0][1][0] != 3) return 13;
    if (cube[0][1][1] != 4) return 14;
    if (cube[1][0][0] != 5) return 15;
    if (cube[1][0][1] != 6) return 16;
    if (cube[1][1][0] != 7) return 17;
    if (cube[1][1][1] != 8) return 18;

    return 0;
}
