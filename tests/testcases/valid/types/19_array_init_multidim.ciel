// Test multidimensional array declaration and access
// Note: Nested initializers like {{1,2,3}, {4,5,6}} are not yet supported

int main() {
    // 2D array declaration and manual initialization
    int matrix[2][3];
    matrix[0][0] = 1;
    matrix[0][1] = 2;
    matrix[0][2] = 3;
    matrix[1][0] = 4;
    matrix[1][1] = 5;
    matrix[1][2] = 6;

    // Validate values
    if (matrix[0][0] != 1) return 1;
    if (matrix[0][2] != 3) return 2;
    if (matrix[1][0] != 4) return 3;
    if (matrix[1][2] != 6) return 4;

    // 2D char array
    char grid[2][5];
    grid[0][0] = 'A';
    grid[0][1] = 'B';
    grid[1][0] = 'X';
    grid[1][1] = 'Y';

    if (grid[0][0] != 'A') return 5;
    if (grid[1][1] != 'Y') return 6;

    return 0;
}
