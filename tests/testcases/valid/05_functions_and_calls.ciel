int takes_array(int arr[3])  { return arr[0] + arr[1] + arr[2]; }

struct RetPoint { int x; int y; };

struct RetPoint make_point(int x, int y) { struct RetPoint p; p.x = x; p.y = y; return p; }

void inc_ptr(struct RetPoint* p) { p->x = p->x + 1; }

char get_grade(int score) {
    if (score >= 90) return 'A';
    if (score >= 80) return 'B';
    if (score >= 70) return 'C';
    if (score >= 60) return 'D';
    return 'F';
}

