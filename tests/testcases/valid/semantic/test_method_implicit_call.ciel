// Test: Method calls with explicit this-> work correctly with access control
class MyClass {
private:
    int value;
    
    void privateHelper() {
        value = 10;
        return;
    }
    
public:
    void publicMethod() {
        this->privateHelper();  // OK: explicit this->privateHelper() - same class
        return;
    }
    
    int getValue() {
        return value;
    }
};

int main() {
    class MyClass obj;
    obj.publicMethod();
    int result = obj.getValue();
    if (result == 10) {
        return 0;
    }
    return 1;
}
