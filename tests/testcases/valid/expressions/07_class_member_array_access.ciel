// Test class member access with arrays

class Vector {
    private:
        float data[3];
        int size;

    public:
        Vector() {
            size = 3;
            data[0] = 0.0;
            data[1] = 0.0;
            data[2] = 0.0;
        }

        float get(int index) {
            return data[index];
        }

        void set(int index, float value) {
            data[index] = value;
            return;
        }

        int get_size() {
            return size;
        }
};

class Matrix {
    private:
        int values[3][3];
        int rows;
        int cols;

    public:
        Matrix() {
            rows = 3;
            cols = 3;
        }

        int get(int row, int col) {
            return values[row][col];
        }

        void set(int row, int col, int value) {
            values[row][col] = value;
            return;
        }
};

class StudentRecord {
    private:
        char name[50];
        int grades[10];
        int num_grades;

    public:
        StudentRecord() {
            num_grades = 0;
        }

        void add_grade(int grade) {
            grades[num_grades] = grade;
            num_grades = num_grades + 1;
            return;
        }

        int get_grade(int index) {
            return grades[index];
        }

        int get_count() {
            return num_grades;
        }
};

int main() {
    // Vector class with array member
    class Vector v;
    v.set(0, 1.5);
    v.set(1, 2.5);
    v.set(2, 3.5);

    float v0 = v.get(0);
    float v1 = v.get(1);
    float v2 = v.get(2);

    // Matrix class with 2D array member
    class Matrix m;
    m.set(0, 0, 1);
    m.set(0, 1, 2);
    m.set(1, 0, 3);
    m.set(2, 2, 9);

    int m00 = m.get(0, 0);
    int m22 = m.get(2, 2);

    // StudentRecord with array operations
    class StudentRecord student;
    student.add_grade(90);
    student.add_grade(85);
    student.add_grade(95);

    int grade0 = student.get_grade(0);
    int grade1 = student.get_grade(1);
    int count = student.get_count();

    // Array of class objects
    class Vector vectors[5];
    vectors[0].set(0, 1.0);
    vectors[1].set(1, 2.0);
    vectors[4].set(2, 5.0);

    float vec0_0 = vectors[0].get(0);
    float vec1_1 = vectors[1].get(1);

    return 0;
}
