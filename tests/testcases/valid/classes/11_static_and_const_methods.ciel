// Test static methods and const methods

class Utility {
    public:
        // Regular methods
        int add(int a, int b) {
            return a + b;
        }

        int subtract(int a, int b) {
            return a - b;
        }

        int multiply(int a, int b) {
            return a * b;
        }

        int divide(int a, int b) {
            return a / b;
        }
};

class Constants {
    public:
        int get_max_int() {
            return 2147483647;
        }

        int get_min_int() {
            return -2147483648;
        }

        float get_pi() {
            return 3.14159;
        }

        float get_e() {
            return 2.71828;
        }
};

class Logger {
    private:
        int log_level;
        char* prefix;

    public:
        Logger(int level) {
            log_level = level;
            prefix = (char *)0;
        }

        void log(char* message) {
            return;
        }

        void error(char* message) {
            return;
        }

        void warning(char* message) {
            return;
        }

        void info(char* message) {
            return;
        }

        int get_level() {
            return log_level;
        }
};

int main() {
    // Test Utility methods
    class Utility util;
    int sum = util.add(10, 20);
    int diff = util.subtract(50, 30);
    int prod = util.multiply(5, 6);
    int quot = util.divide(100, 10);

    // Test Constants methods
    class Constants consts;
    int max = consts.get_max_int();
    int min = consts.get_min_int();
    float pi = consts.get_pi();
    float e = consts.get_e();

    // Test Logger methods
    class Logger logger;
    logger.log((char *)0);
    logger.error((char *)0);
    logger.warning((char *)0);
    logger.info((char *)0);
    int level = logger.get_level();

    return 0;
}
