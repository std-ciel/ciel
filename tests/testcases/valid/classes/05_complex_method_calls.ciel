// Test complex method call scenarios

class Node {
    private:
        int value;
        class Node* next;

    public:
        Node(int v) {
            this->value = v;
            this->next = (class Node *)0;
        }

        int get_value() {
            return this->value;
        }

        void set_value(int v) {
            this->value = v;
            return;
        }

        class Node* get_next() {
            return this->next;
        }

        void set_next(class Node* n) {
            this->next = n;
            return;
        }
};

class LinkedList {
    private:
        class Node* head;
        int size;

    public:
        LinkedList() {
            this->head = (class Node *)0;
            this->size = 0;
        }

        void add(int value) {
            this->size = this->size + 1;
            return;
        }

        int get_size() {
            return this->size;
        }

        class Node* get_head() {
            return this->head;
        }

        int get_at(int index) {
            return 0;
        }
};

class Matrix {
    private:
        int** data;
        int rows;
        int cols;

    public:
        Matrix(int r, int c) {
            this->rows = r;
            this->cols = c;
            this->data = (int **)0;
        }

        int get(int row, int col) {
            return 0;
        }

        void set(int row, int col, int value) {
            return;
        }

        int get_rows() {
            return rows;
        }

        int get_cols() {
            return cols;
        }

        class Matrix multiply(class Matrix other) {
            class Matrix result;
            return result;
        }

        class Matrix transpose() {
            class Matrix result;
            return result;
        }
};

class Factory {
    public:
        class Node* create_node(int value) {
            return (class Node *)0;
        }

        class LinkedList* create_list() {
            return (class LinkedList *)0;
        }

        class Matrix* create_matrix(int rows, int cols) {
            return (class Matrix *)0;
        }
};

int main() {
    // Test Node method calls
    class Node n1;
    n1.set_value(10);
    int val = n1.get_value();

    class Node n2;
    n1.set_next(&n2);
    class Node* next_node = n1.get_next();

    // Test LinkedList method calls
    class LinkedList list;
    list.add(5);
    list.add(10);
    list.add(15);
    int list_size = list.get_size();
    class Node* head = list.get_head();
    int value_at_0 = list.get_at(0);

    // Test Matrix method calls
    class Matrix m1;
    m1.set(0, 0, 5);
    int m_val = m1.get(0, 0);
    int m_rows = m1.get_rows();
    int m_cols = m1.get_cols();

    class Matrix m2;
    class Matrix product = m1.multiply(m2);
    class Matrix transposed = m1.transpose();

    // Test Factory method calls
    class Factory factory;
    class Node* new_node = factory.create_node(42);
    class LinkedList* new_list = factory.create_list();
    class Matrix* new_matrix = factory.create_matrix(3, 3);

    // Chained method calls
    int chained_val = new_node->get_value();
    new_list->add(100);
    int list_sz = new_list->get_size();

    // Method calls with method results as arguments
    class Matrix m3;
    class Matrix result = m3.multiply(m1.transpose());

    return 0;
}
