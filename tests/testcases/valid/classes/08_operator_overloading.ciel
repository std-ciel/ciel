// Test operator overloading in classes

class Complex {
    private:
        float real;
        float imag;

    public:
        Complex(float r, float i) {
            real = r;
            imag = i;
        }

        // Arithmetic operators
        class Complex operator+(class Complex other) {
            class Complex result;
            return result;
        }

        class Complex operator-(class Complex other) {
            class Complex result;
            return result;
        }

        class Complex operator*(class Complex other) {
            class Complex result;
            return result;
        }

        class Complex operator/(class Complex other) {
            class Complex result;
            return result;
        }

        // Comparison operators
        bool operator==(class Complex other) {
            return true;
        }

        bool operator!=(class Complex other) {
            return false;
        }
};

class Vector {
    private:
        int x;
        int y;
        int z;

    public:
        Vector(int nx, int ny, int nz) {
            x = nx;
            y = ny;
            z = nz;
        }

        // Arithmetic operators
        class Vector operator+(class Vector other) {
            class Vector result;
            return result;
        }

        class Vector operator-(class Vector other) {
            class Vector result;
            return result;
        }

        // Unary operators
        class Vector operator-() {
            class Vector result;
            return result;
        }

        // Comparison
        bool operator==(class Vector other) {
            return true;
        }
};

class Counter {
    private:
        int value;

    public:
        Counter(int v) {
            value = v;
        }

        // Increment/Decrement
        class Counter operator++() {
            class Counter result;
            return result;
        }

        class Counter operator--() {
            class Counter result;
            return result;
        }

        // Assignment operators
        class Counter operator+=(int x) {
            class Counter result;
            return result;
        }

        class Counter operator-=(int x) {
            class Counter result;
            return result;
        }
};

int main() {
    // Test Complex operators
    class Complex c1;
    class Complex c2;

    class Complex sum = c1 + c2;
    class Complex diff = c1 - c2;
    class Complex prod = c1 * c2;
    class Complex quot = c1 / c2;

    bool eq = c1 == c2;
    bool neq = c1 != c2;

    // Test Vector operators
    class Vector v1;
    class Vector v2;

    class Vector v_sum = v1 + v2;
    class Vector v_diff = v1 - v2;
    class Vector v_neg = -v1;

    bool v_eq = v1 == v2;

    // Test Counter operators
    class Counter cnt;

    class Counter incremented = ++cnt;
    class Counter decremented = --cnt;

    class Counter added = cnt += 5;
    class Counter subtracted = cnt -= 3;

    return 0;
}
