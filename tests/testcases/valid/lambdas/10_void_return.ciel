// Test: Lambda with void return type
// Expected: Both constructors and void operator() work correctly
int printf(char* format, ...);

int main() {
    printf("Test: Lambda with void return type\n");
    
    int counter;
    counter = 0;

    // Lambda with void return
    [] do_nothing() -> void {
        return;
    };

    // Lambda with capture and void return
    [counter = counter] reset() -> void {
        return;
    };

    // Call void lambdas
    do_nothing();
    printf("do_nothing() called\n");
    
    reset();
    printf("reset() called\n");
    printf("Expected: Both void lambdas called successfully\n");

    return 0;
}
