// Test: Lambda return values used in expressions
// Expected: operator() return value correctly used in arithmetic
int printf(char* format, ...);

int main() {
    printf("Test: Lambda return values used in expressions\n");
    
    int a;
    int b;

    a = 10;
    b = 20;

    // Lambdas that return values
    [a = a] get_a() -> int {
        return a;
    };

    [b = b] get_b() -> int {
        return b;
    };

    [] calculate(int x, int y) -> int {
        return x + y;
    };

    // Use lambda return values in expressions
    int sum = get_a() + get_b();
    printf("Sum (get_a + get_b): %d\n", sum);
    printf("Expected: 30\n");
    
    int result = calculate(5, 10) + get_a();
    printf("Result (calculate(5,10) + get_a): %d\n", result);
    printf("Expected: 25\n");

    return 0;
}
