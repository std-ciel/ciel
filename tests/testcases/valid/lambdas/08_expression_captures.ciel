// Test: Lambda with expression captures
// Expected: Capture expressions evaluated at lambda creation
int printf(char* format, ...);

int main() {
    printf("Test: Lambda with expression captures\n");
    
    int x;
    int y;

    x = 5;
    y = 3;

    // Lambda with expression captures
    [sum = x + y, product = x * y] compute(int n) -> int {
        return sum + product + n;
    };

    // Call the lambda
    int result = compute(2);
    printf("Result: %d\n", result);
    printf("Expected: 25 (sum=8, product=15, n=2, total=8+15+2=25)\n");

    return 0;
}
