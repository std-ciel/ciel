// Test: Lambda with multiple captures and no parameters
// Expected: Constructor should be called with all capture values
int printf(char* format, ...);

int main() {
    printf("Test: Lambda with multiple captures\n");
    
    int x;
    int y;
    int z;

    x = 10;
    y = 20;
    z = 30;

    // Lambda with multiple captures
    [x = x, y = y, z = z] sum_all() -> int {
        return x + y + z;
    };

    // Call the lambda
    int result = sum_all();
    printf("Result: %d\n", result);
    printf("Expected: 60\n");

    return 0;
}
