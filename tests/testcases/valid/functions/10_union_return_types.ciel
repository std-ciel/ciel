// Test union return types

union Data {
    int i;
    float f;
    char c;
};

union Value {
    int intval;
    float floatval;
    char* strval;
};

// Return union by value
union Data create_data() {
    union Data d;
    return d;
}

union Value create_value() {
    union Value v;
    return v;
}

// Return pointer to union
union Data* alloc_data() {
    return (union Data*)0;
}

union Value* create_value_ptr() {
    return (union Value*)0;
}

// Return double pointer to union
union Data** create_data_array() {
    return (union Data**)0;
}

// Functions with union parameters
union Data process_data(union Data d) {
    return d;
}

union Value* modify_value(union Value* v) {
    return v;
}

int main() {
    return 0;
}
