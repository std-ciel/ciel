// Test basic return types

// Void return
void func_void() {
    return;
}

// Basic integer types
int func_int() {
    return 42;
}

int func_signed() {
    return -10;
}

unsigned func_unsigned() {
    return (unsigned)100;
}

// Character types
char func_char() {
    return 'a';
}

// Boolean type
bool func_bool() {
    return true;
}

// Floating point type
float func_float() {
    return 3.14;
}

int main(){
    func_void();

    // Validate return values
    if (func_int() != 42) return 1;
    if (func_signed() != -10) return 2;
    if ((int)func_unsigned() != 100) return 3;
    if (func_char() != 'a') return 4;
    if (!func_bool()) return 5;
    if (func_float() != 3.14) return 6;

    return 0;
}
