// Test single-level pointer return types

// Pointer to basic types
int* func_int_ptr() {
    return (int*)0;
}

char* func_char_ptr() {
    return (char*)0;
}

float* func_float_ptr() {
    return (float*)0;
}

bool* func_bool_ptr() {
    return (bool*)0;
}

unsigned* func_unsigned_ptr() {
    return (unsigned*)0;
}

int* func_signed_ptr() {
    return (int*)0;
}

void* func_void_ptr() {
    return (void*)0;
}

int main(){
    // Validate pointer returns (all should be null pointers)
    if (func_int_ptr() != (int*)0) return 1;
    if (func_char_ptr() != (char*)0) return 2;
    if (func_float_ptr() != (float*)0) return 3;
    if (func_bool_ptr() != (bool*)0) return 4;
    if (func_unsigned_ptr() != (unsigned*)0) return 5;
    if (func_signed_ptr() != (int*)0) return 6;
    if (func_void_ptr() != (void*)0) return 7;

    return 0;
}
