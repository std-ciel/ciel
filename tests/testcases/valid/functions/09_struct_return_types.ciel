// Test struct return types

struct Point {
    int x;
    int y;
};

struct Vector {
    float x;
    float y;
    float z;
};

struct Node {
    int data;
    int* next;
};

// Return struct by value
struct Point create_point() {
    struct Point p;
    return p;
}

struct Vector create_vector() {
    struct Vector v;
    return v;
}

// Return pointer to struct
struct Point* alloc_point() {
    return (struct Point*)0;
}

struct Node* create_node() {
    return (struct Node*)0;
}

// Return double pointer to struct
struct Point** create_point_array() {
    return (struct Point**)0;
}

// Functions with struct parameters
struct Point transform_point(struct Point p) {
    return p;
}

struct Vector* process_vector(struct Vector* v) {
    return v;
}

