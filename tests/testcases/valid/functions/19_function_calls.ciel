// Test function calls with various signatures

// Basic function calls
int add(int a, int b) {
    return a + b;
}

float multiply(float a, float b) {
    return a * b;
}

void print_value(int val) {
    return;
}

// Functions with pointer parameters
int* create_array(int size) {
    return (int *)0;
}

void fill_array(int* arr, int size, int value) {
    return;
}

// Functions with struct parameters
struct Point {
    int x;
    int y;
};

struct Point make_point(int px, int py) {
    struct Point p;
    return p;
}

int distance(struct Point p1, struct Point p2) {
    return 0;
}

// Functions with multiple pointer levels
int** create_matrix(int rows, int cols) {
    return (int **)0;
}

void free_matrix(int** matrix, int rows) {
    return;
}

// Variadic function calls
int sum(int count, ...) {
    return 0;
}

char* format_string(char* fmt, ...) {
    return (char *)0;
}

// Function returning pointer
char* get_name() {
    return (char *)0;
}

// Nested function calls
int compute(int a, int b, int c) {
    return a + b + c;
}

int main() {
    // Basic calls
    int result1 = add(5, 10);
    float result2 = multiply(3.14, 2.0);
    print_value(42);

    // Pointer returns
    int* arr = create_array(10);
    fill_array(arr, 10, 5);

    // Struct calls
    struct Point p1 = make_point(0, 0);
    struct Point p2 = make_point(10, 20);
    int dist = distance(p1, p2);

    // Matrix calls
    int** matrix = create_matrix(5, 5);
    free_matrix(matrix, 5);

    // Variadic calls
    int total = sum(3, 10, 20, 30);
    char* msg = format_string("Hello %s", "world");

    // Nested calls
    int nested = compute(add(1, 2), (int)multiply(3.0, 4.0), 5);

    // Call with function result as parameter
    print_value(add(10, 20));

    // Multiple calls in expression
    int complex = add(add(1, 2), add(3, 4));

    // Call returning pointer used in another call
    char* name = get_name();

    return 0;
}
